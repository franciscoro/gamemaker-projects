            <kind>1</kind>
            <string>/// handle movement


right = keyboard_check(vk_right)
left = keyboard_check(vk_left)
up = keyboard_check_pressed(vk_up)
down = keyboard_check(vk_down)

max_gravity = 15

if (right) {
    _hspeed = move_speed
    image_xscale = 1
    
}

if (left) {
    _hspeed = -move_speed
    image_xscale = -1
    
}

if (left and right) {
    _hspeed = 0
 
}
if (not left and not  right and on_ground) {
    _hspeed -= sign(_hspeed) * _friction
}

if (_vspeed &lt; max_gravity) _vspeed += _gravity

//always check this before anything else
if (place_meeting(x,y+1,parent_block))
{
    on_ground = true
    _vspeed = up * -jump_speed
  } else {
    on_ground = false
}


//handle horiz 
if ( place_meeting(x + _hspeed, y , parent_block) )  {
    while (not place_meeting(x + sign(_hspeed),y, parent_block) ) {
        x += sign(_hspeed)
    }
    _hspeed = 0
    
} 
x += _hspeed

//handle vert
if ( place_meeting(x, y+_vspeed , parent_block) )  {
    while (not place_meeting(x,y + sign(_vspeed), parent_block) ) {
        y += sign(_vspeed)
    }
    _vspeed = 0
} 
y += _vspeed

if (_hspeed == 0) {
    sprite_index = spr_player
} else {
    sprite_index = spr_player_eft    
}

if (y &gt; room_height) { 
    instance_destroy()
    
}


</string>
